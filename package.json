{
  "name": "backbone-dropdown",
  "version": "2.0.0",
  "description": "Extensible dropdown component for Backbone.js apps.",
  "main": "src/dropdown.js",
  "scripts": {
    "start": "parcel demo/index.html --out-dir demo-build",
    "build": "parcel build demo/index.html --out-dir demo-build",
    "gh-pages-build": "parcel build demo/index.html --out-dir demo-build --public-url ./",
    "gh-pages-deploy": "node node_modules/push-dir/bin/push-dir.js --dir=demo-build --branch=gh-pages --cleanup",
    "gh-pages": "parcel build demo/index.html --out-dir demo-build --public-url ./ && node node_modules/push-dir/bin/push-dir.js --dir=demo-build --branch=gh-pages --cleanup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjantzer/backbone-dropdown"
  },
  "keywords": [
    "dropdown",
    "tooltip",
    "menu",
    "select"
  ],
  "author": "Kevin Jantzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjantzer/backbone-dropdown/issues"
  },
  "homepage": "https://github.com/kjantzer/backbone-dropdown",
  "devDependencies": {
    "parcel-bundler": "^1.9.6",
    "basic-buttons": "^1.2.0",
    "push-dir": "^0.4.1"
  },
  "dependencies": {
    "backbone": "0.9.10",
    "jquery": "^3.2.0",
    "liquidmetal": "^1.3.0",
    "underscore": "^1.8.3"
  }
}
